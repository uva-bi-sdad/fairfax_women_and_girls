  "metadata": {
    "url": "",
    "package": "data/datapackage.json",
    "datasets": ["block_group", "tract", "planning_district", "supervisor_district", "human_services_region", "county"],
    "variables": {},
    "entity_info": {},
    "files": ["block_group.csv.xz", "tract.csv.xz", "planning_district.csv.xz", "supervisor_district.csv.xz", "human_services_region.csv.xz", "county.csv.xz"]
  },
  "rules": [
    {
      "condition": [
        {
          "id": "selected_tract",
          "type": "!",
          "value": "",
          "any": false
        }
      ],
      "effects": {
        "lock": "sec31"
      }
    },
    {
      "condition": [
        {
          "id": "shapes",
          "type": "=",
          "value": "tract",
          "any": true
        },
        {
          "id": "shapes",
          "type": "=",
          "value": "block_group",
          "any": true
        }
      ],
      "effects": {
        "display": "sec32"
      }
    },
    {
      "condition": [
        {
          "id": "selected_tract",
          "type": "!",
          "value": "",
          "any": false
        }
      ],
      "effects": {
        "lock": "menu21"
      }
    }
  ],
  "variables": [
    {
      "id": "shapes",
      "states": [
        {
          "condition": [
            {
              "id": "selected_county",
              "type": "",
              "value": "",
              "any": false
            },
            {
              "id": "shape_type",
              "type": "=",
              "value": "county",
              "any": false
            }
          ],
          "value": "tract"
        },
        {
          "condition": [
            {
              "id": "selected_tract",
              "type": "",
              "value": "",
              "any": false
            }
          ],
          "value": "block_group"
        }
      ],
      "default": "shape_type",
      "display": {
        "county": "Counties",
        "tract": "Census Tracts",
        "block_group": "Block Groups",
        "supervisor_district": "Supervisor Districts",
        "planning_district": "Planning Districts",
        "human_services_region": "Human Services Regions",
        "zip_code": "Zip Codes"
      }
    },
    {
      "id": "region_select",
      "states": [
        {
          "condition": [
            {
              "id": "shapes",
              "type": "=",
              "value": "county",
              "any": false
            }
          ],
          "value": "selected_county"
        }
      ],
      "default": "selected_tract",
      "display": []
    },
    {
      "id": "selected_region",
      "states": [
        {
          "condition": [
            {
              "id": "selected_tract",
              "type": "",
              "value": "",
              "any": false
            }
          ],
          "value": "selected_tract"
        }
      ],
      "default": "selected_county",
      "display": []
    },
    {
      "id": "set_palette",
      "states": [
        {
          "condition": [
            {
              "id": "settings.color_by_order",
              "type": "",
              "value": "",
              "any": false
            }
          ],
          "value": "lajolla"
        }
      ],
      "default": "vik",
      "display": []
    },
    {
      "id": "tract_subset",
      "states": [
        {
          "condition": [
            {
              "id": "selected_county",
              "type": "",
              "value": "",
              "any": false
            }
          ],
          "value": "siblings"
        }
      ],
      "default": "full_filter",
      "display": []
    }
  ],
  "dataviews": {
    "primary_view": {
      "palette": "set_palette",
      "y": "selected_variable",
      "x": "selected_year",
      "time": "time",
      "time_agg": "selected_year",
      "time_filters": [],
      "dataset": "shapes",
      "ids": "selected_region"
    }
  },
