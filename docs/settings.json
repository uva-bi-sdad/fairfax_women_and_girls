    "table_autoscroll": true,
    "table_scroll_behavior": "smooth",
    "table_autosort": true,
    "hide_tooltips": false,
    "map_animations": "all",
    "trace_limit": 20,
    "map_overlay": true,
    "circle_radius": 7,
    "tracking": false,
    "show_empty_times": false
