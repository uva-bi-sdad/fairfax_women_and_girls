  "button": {
    "DownloadAllData": {
      "effects": "export",
      "dataview": {},
      "query": {
        "features": {
          "geoid": "id",
          "name": "name"
        }
      },
      "api": false
    },
    "Download": {
      "effects": "export",
      "dataview": "primary_view",
      "query": {
        "include": "selected_variable",
        "features": {
          "geoid": "id",
          "name": "name"
        },
        "table_format": "export_table_format",
        "file_format": "export_file_format"
      },
      "api": false
    },
    "CopyAPIlink": {
      "effects": "copy",
      "dataview": "primary_view",
      "query": {
        "include": "selected_variable",
        "dataset": "shapes",
        "features": {
          "geoid": "id",
          "name": "name"
        },
        "table_format": "export_table_format",
        "file_format": "export_file_format"
      },
      "api": false
    }
  },
